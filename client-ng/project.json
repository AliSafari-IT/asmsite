{
  "name": "client-ng",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "client-ng/src",
  "tags": [
    "ASAFARIM"
  ],
  "targets": {
    "eslint:lint": {
      "cache": true,
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/.eslintrc.json",
        "{projectRoot}/.eslintrc.json",
        "{workspaceRoot}/tools/eslint-rules/**/*",
        {
          "externalDependencies": [
            "eslint"
          ]
        }
      ],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "client-ng",
        "command": "eslint ."
      },
      "configurations": {}
    },
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/client-ng/browser",
        "index": "client-ng/src/index.html",
        "main": "client-ng/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "client-ng/tsconfig.app.json",
        "assets": [
          "client-ng/src/favicon.ico",
          "client-ng/src/assets"
        ],
        "styles": [
          "client-ng/src/styles.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "client-ng:build:production"
        },
        "development": {
          "buildTarget": "client-ng:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "client-ng/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "client-ng:build"
      },
      "configurations": {}
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {},
      "configurations": {}
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "client-ng/jest.config.ts"
      },
      "configurations": {}
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "client-ng:build",
        "port": 4200,
        "spa": true
      },
      "configurations": {}
    },
    "server": {
      "dependsOn": [
        "build"
      ],
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/client-ng/server",
        "main": "client-ng/server.ts",
        "tsConfig": "client-ng/tsconfig.server.json"
      },
      "configurations": {
        "production": {
          "outputHashing": "media"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false,
          "vendorChunk": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "executor": "@angular-devkit/build-angular:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "client-ng:build:development",
          "serverTarget": "client-ng:server:development"
        },
        "production": {
          "browserTarget": "client-ng:build:production",
          "serverTarget": "client-ng:server:production"
        }
      },
      "defaultConfiguration": "development",
      "options": {}
    },
    "prerender": {
      "executor": "@angular-devkit/build-angular:prerender",
      "options": {
        "routes": [
          "/"
        ]
      },
      "configurations": {
        "development": {
          "browserTarget": "client-ng:build:development",
          "serverTarget": "client-ng:server:development"
        },
        "production": {
          "browserTarget": "client-ng:build:production",
          "serverTarget": "client-ng:server:production"
        }
      },
      "defaultConfiguration": "production"
    }
  },
  "implicitDependencies": []
}