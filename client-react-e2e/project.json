          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress"
        ],
        "description": "Runs Cypress Tests in src/e2e/app.cy.ts in CI"
      },
      "executor": "nx:run-commands",
      "options": {
        "cwd": "client-react-e2e",
        "command": "cypress run --env webServerCommand=\"nx run client-react:serve-static\" --spec src/e2e/app.cy.ts"
      },
      "configurations": {}
    },
    "e2e-ci": {
      "executor": "nx:noop",
      "cache": true,
      "inputs": [
        "default",
        "^default",
        {
          "externalDependencies": [
            "cypress"
          ]
        }
      ],
      "outputs": [
        "{workspaceRoot}/dist/cypress/client-react-e2e/videos",
        "{workspaceRoot}/dist/cypress/client-react-e2e/screenshots"
      ],
      "dependsOn": [
        {
          "target": "e2e-ci--src/e2e/app.cy.ts",
          "projects": "self",
          "params": "forward"
        }
      ],
