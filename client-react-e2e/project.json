{
  "name": "client-react-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "client-react-e2e/src",
  "tags": [],
  "implicitDependencies": [
    "client-react"
  ],
  "// targets": "to see all targets run: nx show project client-react-e2e --web",
  "targets": {
    "eslint:lint": {
      "cache": true,
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/.eslintrc.json",
        "{projectRoot}/.eslintrc.json",
        "{workspaceRoot}/tools/eslint-rules/**/*",
        {
          "externalDependencies": [
            "eslint"
          ]
        }
      ],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "client-react-e2e",
        "command": "eslint ."
      },
      "configurations": {}
    },
    "e2e": {
      "cache": true,
      "inputs": [
        "default",
        "^default",
        {
          "externalDependencies": [
            "cypress"
          ]
        }
      ],
      "outputs": [
        "{workspaceRoot}/dist/cypress/client-react-e2e/videos",
        "{workspaceRoot}/dist/cypress/client-react-e2e/screenshots"
      ],
      "metadata": {
        "technologies": [
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress"
        ],
        "description": "Runs Cypress Tests"
      },
      "executor": "nx:run-commands",
      "options": {
        "cwd": "client-react-e2e",
        "command": "cypress run"
      },
      "configurations": {
        "production": {
          "command": "cypress run --env webServerCommand=\"nx run client-react:preview\""
        }
      }
    },
    "e2e-ci--src/e2e/app.cy.ts": {
      "outputs": [
        "{workspaceRoot}/dist/cypress/client-react-e2e/videos",
        "{workspaceRoot}/dist/cypress/client-react-e2e/screenshots"
      ],
      "inputs": [
        "default",
        "^default",
        {
          "externalDependencies": [
            "cypress"
          ]
        }
      ],
      "cache": true,
      "metadata": {
        "technologies": [
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
          "cypress",
