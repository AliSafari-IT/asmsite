    "start:ng": "nx serve client-ng --open",
    "start:react": "nx serve client-react --open",
    "start:docs": "nx run client-docs:serve --open",
    "build:ng": "nx build client-ng  --base-href=/dist/client-ng/browser/",
    "build:react": "npx nx build client-react",
    "test": "nx test client-ng",
    "lint": "nx lint client-ng",
    "do": "npm run lint && npm run build && npm run test",
