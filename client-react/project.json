{
  "name": "client-react",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "client-react/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project client-react --web",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "options": {
        "outputPath": "dist/client-react",
        "index": "apps/client-react/src/index.html",
        "entryFile": "apps/client-react/src/main.tsx",
        "tsConfig": "apps/client-react/tsconfig.app.json",
        "assets": [
          "apps/client-react/src/favicon.ico",
          "apps/client-react/src/assets"
        ],
        "styles": [
          "apps/client-react/src/styles.css"
        ]
      },
      "configurations": {}
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "options": {
        "buildTarget": "client-react:build",
        "port": 4200,
        "open": true
      },
      "configurations": {}
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "client-react",
        "command": "vite preview"
      },
      "configurations": {}
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "build",
        "spa": true
      },
      "configurations": {}
    },
    "test": {
      "cache": true,
      "inputs": [
        "default",
        "^default",
        {
          "externalDependencies": [
            "vitest"
          ]
        }
      ],
      "outputs": [
        "{workspaceRoot}\\coverage\\client-react"
      ],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "client-react",
        "command": "vitest run"
      },
      "configurations": {}
    },
    "eslint:lint": {
      "cache": true,
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/.eslintrc.json",
        "{projectRoot}/.eslintrc.json",
        "{workspaceRoot}/tools/eslint-rules/**/*",
        {
          "externalDependencies": [
            "eslint"
          ]
        }
      ],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "client-react",
        "command": "eslint ."
      },
      "configurations": {}
    }
  },
  "implicitDependencies": []
}